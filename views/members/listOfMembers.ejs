<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="UTF-8">
<title>Explore Our Members</title>
</head>
<body>
    <nav>
        <a href="/">Home..</a>
        <% if(user) { %>
          <a href="/members/">Explore Other Pantrys..</a>
          <a href="/users/<%=user._id%>/foods">View my Pantry</a>
          <a href="/auth/sign-out">Sign Out</a>
        <% } else { %>
          <a href="/auth/sign-in">Sign In</a>
          <a href="/auth/sign-up">Sign Up</a>
        <% } %>
      </nav>
  <h1>Explore Our Members</h1>
  <h2>List of Memebers</h2>
  <section>
    <div>
   <ul>
    <% members.forEach(member => { %>
      <div>
        <li><a href="/members/<%= user._id %>"><%= member.username %></a></li>
      </div>
    <% }); %>
   </ul>
</div>
</section>
<button><a href="/">Back to Pantry</a></button>
</body>
</html>